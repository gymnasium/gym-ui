{
  "name": "@gymnasium/gym-ui",
  "version": "0.0.25",
  "description": "React UI Component Library used by Gymnasium sites and tools",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist",
    "compile": "npm run clean && tsc -p .",
    "lint": "eslint src --ext .jsx,.js,.tsx,.ts",
    "prepublishOnly": "npm run compile",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "release": "npm run compile && npm publish --access public",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280",
    "start": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "watch": "tsc -w -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gymnasium/gym-ui.git"
  },
  "keywords": [
    "gymnasium",
    "ui",
    "library",
    "gym-ui",
    "react"
  ],
  "author": "Mike Bifulco <mbifulco@aquent.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/gymnasium/gym-ui/issues"
  },
  "homepage": "https://github.com/gymnasium/gym-ui#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@percy/storybook": "^3.3.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "@types/node": "^14.0.10",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "prettier": "2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "source-map-loader": "^1.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
