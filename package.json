{
  "name": "@gymnasium/gym-ui",
  "version": "0.0.6",
  "description": "React UI Component Library used by Gymnasium sites and tools",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint src --ext .jsx,.js,.tsx,.ts",
    "release": "npm publish --access public",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "watch": "tsc -w -p .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gymnasium/gym-ui.git"
  },
  "keywords": [
    "gymnasium",
    "ui",
    "library",
    "gym-ui",
    "react"
  ],
  "author": "Mike Bifulco <mbifulco@aquent.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/gymnasium/gym-ui/issues"
  },
  "homepage": "https://github.com/gymnasium/gym-ui#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-actions": "^5.2.0",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/storybook-addon-jsx": "^5.4.3",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "prettier": "1.18.2",
    "source-map-loader": "^0.2.4",
    "storybook-addon-jsx": "^7.1.6",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "dist/components"
  ]
}
