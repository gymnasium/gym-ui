{
  "name": "@gymnasium/gym-ui",
  "version": "0.0.17",
  "description": "React UI Component Library used by Gymnasium sites and tools",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist",
    "compile": "npm run clean && tsc -p .",
    "lint": "eslint src --ext .jsx,.js,.tsx,.ts",
    "prepublishOnly": "npm run compile",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "release": "npm run compile && npm publish --access public",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280",
    "start": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "watch": "tsc -w -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gymnasium/gym-ui.git"
  },
  "keywords": [
    "gymnasium",
    "ui",
    "library",
    "gym-ui",
    "react"
  ],
  "author": "Mike Bifulco <mbifulco@aquent.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/gymnasium/gym-ui/issues"
  },
  "homepage": "https://github.com/gymnasium/gym-ui#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@percy/storybook": "^3.2.0",
    "@storybook/addon-a11y": "^5.2.5",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-backgrounds": "^5.2.5",
    "@storybook/addon-docs": "^5.3.0-alpha.41",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@storybook/theming": "^5.2.5",
    "@types/node": "^12.12.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-typescript": "^6.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "prettier": "1.18.2",
    "react-docgen-typescript-loader": "^3.3.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
