{
  "name": "@gymnasium/gym-ui",
  "version": "0.0.18",
  "description": "React UI Component Library used by Gymnasium sites and tools",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist",
    "compile": "npm run clean && tsc -p .",
    "lint": "eslint src --ext .jsx,.js,.tsx,.ts",
    "prepublishOnly": "npm run compile",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "release": "npm run compile && npm publish --access public",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280",
    "start": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "watch": "tsc -w -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gymnasium/gym-ui.git"
  },
  "keywords": [
    "gymnasium",
    "ui",
    "library",
    "gym-ui",
    "react"
  ],
  "author": "Mike Bifulco <mbifulco@aquent.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/gymnasium/gym-ui/issues"
  },
  "homepage": "https://github.com/gymnasium/gym-ui#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@percy/storybook": "^3.2.0",
    "@storybook/addon-a11y": "^5.3.6",
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-backgrounds": "^5.3.6",
    "@storybook/addon-docs": "^5.3.6",
    "@storybook/addon-info": "^5.3.8",
    "@storybook/addon-links": "^5.3.6",
    "@storybook/addons": "^5.3.6",
    "@storybook/react": "^5.3.6",
    "@storybook/theming": "^5.3.6",
    "@types/node": "^13.1.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-flowtype": "^4.5.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "prettier": "1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
